--gameover
function gameover()
	clearenemies()
	if (hp==0)	sfx(4,1)
	hp-=1
	if (points>dget(highscore)) dset(highscore,points)
end
-->8
--scoreboard
function drawscoreboard()
	tc=6
	print("\npnts",tc)
	print(points,tc)
	print("\nhigh",tc)
	print(dget(higscore),tc)
	if (hp<1)	print("\ngame\nover")
end
-->8
--restart
function restart()
	clearenemies()
	val=0
	ticker=0
	points=0
	hp=3
end
